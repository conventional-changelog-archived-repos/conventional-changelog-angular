  },
  "homepage": "https://github.com/conventional-changelog/conventional-changelog-angular#readme",
  "devDependencies": {
    "better-than-before": "^1.0.0",
    "chai": "^3.5.0",
    "conventional-changelog-core": "^1.5.0",
    "coveralls": "^2.11.14",
    "git-dummy-commit": "^1.3.0",
    "istanbul": "^0.4.5",
    "jscs": "^3.0.7",
    "jshint": "^2.9.4",
    "mocha": "*",
    "shelljs": "^0.7.5",
    "through2": "^2.0.1"
